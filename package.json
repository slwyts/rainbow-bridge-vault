{
  "name": "rainbow-bridge-vault",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e env/global.env -e env/localnet.env -e env/bsc-testnet.env -e env/bsc.env -e env/xlayer.env -e env/arbitrum.env -- next dev",
    "dev:local": "node scripts/start-local.js",
    "build": "npx hardhat compile && dotenv -e env/global.env -e env/localnet.env -e env/bsc-testnet.env -e env/bsc.env -e env/xlayer.env -e env/arbitrum.env -- next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "test": "hardhat test",
    "node": "hardhat node",
    "time": "node scripts/time-jump.js",
    "deploy:tbsc": "dotenv -e env/bsc-testnet.env -- hardhat run scripts/deploy-tbsc.ts --network bscTestnet",
    "deploy": "node scripts/deploy.cjs"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@radix-ui/react-alert-dialog": "1.1.15",
